  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "npm-force-resolutions": "0.0.3",
    "popper.js": "^1.16.1"
  }
}
